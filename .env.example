# Telegram API credentials (get from https://my.telegram.org/)
TELEGRAM_API_ID=your_api_id
TELEGRAM_API_HASH=your_api_hash

# Phone number (for initial setup only - not needed in Coolify)
TELEGRAM_PHONE=+12345678901

# Session string (generate using GENERATE_SESSION=yes)
TELEGRAM_SESSION_STRING=your_session_string

# Optional: Log channel ID or username
LOG_CHANNEL_ID=your_log_channel

# Set to "yes" to generate a session string (for local use only)
# GENERATE_SESSION=yes 