# Telegram API credentials (get from https://my.telegram.org/apps)
TELEGRAM_API_ID=123456
TELEGRAM_API_HASH=abcdef1234567890abcdef1234567890

# Phone number (for initial setup only - not needed in Coolify)
TELEGRAM_PHONE=+12345678901

# Session string (run locally with GENERATE_SESSION=yes first)
# Then add the output string here for Coolify deployment
TELEGRAM_SESSION_STRING=

# Log channel ID (optional)
LOG_CHANNEL_ID=-100123456789

# Set to "yes" to generate a session string (for local use only)
# GENERATE_SESSION=yes 